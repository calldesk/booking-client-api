{
  "name": "client-api",
  "version": "0.1.4",
  "description": "Mocked client API for demo",
  "main": "lib/index.js",
  "scripts": {
    "test": "semistandard && flow check",
    "doc": "apidoc -i src/",
    "compile": "babel -d lib/ src/",
    "version": "npm run compile && npm run doc && git add lib",
    "postversion": "git push origin master && git push origin master --tags && ./scripts/uploadS3.sh",
    "prestart": "npm run doc",
    "start": "node lib/index.js",
    "dev-start": "babel-node src/index.js"
  },
  "pre-commit": [
    "doc",
    "test"
  ],
  "dependencies": {
    "apidoc": "^0.15.1",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "moment": "^2.13.0",
    "twilio": "^2.9.1",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "chai": "^3.5.0",
    "nock": "^8.0.0",
    "pre-commit": "^1.1.2",
    "sinon": "^1.17.4"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [],
    "ignore": [
      "lib/*",
      "doc/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/booking-client-api"
  },
  "author": "Vincent Gire",
  "license": "ISC"
}

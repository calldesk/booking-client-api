{
  "name": "booking-client-api",
  "version": "0.1.16",
  "description": "Mocked client API for demo",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run syntax && mocha --compilers js:babel-register",
    "syntax": "semistandard && flow check",
    "doc": "apidoc -i src/",
    "install": "babel -d lib/ src/",
    "preversion": "npm run coverage && npm run doc",
    "postversion": "git push origin master && git push origin master --tags && git push -f origin master:gh-pages",
    "prestart": "npm run doc",
    "start": "node lib/index.js",
    "coverage": "babel-istanbul cover _mocha && babel-istanbul check-coverage --statement 90",
    "dev-start": "babel-node src/index.js"
  },
  "pre-commit": [
    "syntax"
  ],
  "dependencies": {
    "apidoc": "^0.15.1",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "twilio": "^2.9.1"
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "expect.js": "^0.3.1",
    "nock": "^8.0.0",
    "pre-commit": "^1.1.2",
    "sinon": "^1.17.4"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [],
    "ignore": [
      "lib/*",
      "doc/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calldesk/booking-client-api"
  },
  "author": "Vincent Gire",
  "license": "ISC"
}
